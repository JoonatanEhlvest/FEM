{
  "name": "fem-viewer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.0.1",
    "@testing-library/user-event": "14.1.0",
    "axios": "0.26.1",
    "fast-xml-parser": "4.0.7",
    "path": "^0.12.7",
    "rc-slider": "10.0.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-draggable": "^4.4.4",
    "react-resizable": "^3.0.4",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.0",
    "reactjs-popup": "^2.0.5",
    "typescript": "4.6.3",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "start:dev": "PORT=3001 react-scripts start",
    "start:dev:all": "concurrently \"yarn start:dev\" \"cd server && yarn start:dev\"",
    "db:start": "cd server && yarn db:start",
    "db:stop": "cd server && yarn db:stop",
    "build": "react-scripts build",
    "test": "cd server && yarn test",
    "test:setup": "cd server && yarn test:setup",
    "test:teardown": "cd server && yarn test:teardown",
    "eject": "react-scripts eject",
    "start": "NODE_ENV=production yarn run build && (cd server && yarn start)",
    "dtm": "dependency-time-machine --update --install --install-script 'yarn install' --auto --test-script 'yarn test'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/rc-slider": "^9.3.1",
    "@types/react": "17.0.43",
    "@types/react-dom": "18.0.0",
    "@types/react-resizable": "^1.7.4",
    "concurrently": "^9.1.2",
    "dependency-time-machine": "^1.3.4"
  },
  "proxy": "http://localhost:3000"
}
