{
	"compilerOptions": {
		"target": "es5",
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noFallthroughCasesInSwitch": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "react-jsx",
		"outDir": "build",
		"baseUrl": ".",
		"paths": {
			"*": ["node_modules/*"]
		}
	},
	"include": ["./src/**/*"]
}
