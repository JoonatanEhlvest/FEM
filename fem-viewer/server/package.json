{
	"name": "server",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/cookie-parser": "^1.4.2",
		"@types/express": "^4.17.13",
		"@types/express-mysql-session": "^2.1.3",
		"@types/express-session": "^1.17.4",
		"@types/multer": "^1.4.7",
		"@types/node": "^17.0.21",
		"@types/passport": "^1.0.7",
		"@types/passport-local": "^1.0.34",
		"express": "^4.17.3",
		"nodemon": "^2.0.15",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.2",
		"@types/jest": "^27.4.1",
		"@types/supertest": "^2.0.11",
		"jest": "^27.5.1",
		"supertest": "^6.2.2",
		"ts-jest": "^27.1.3",
		"cross-env": "^7.0.3"
	},
	"scripts": {
		"build": "tsc --p .",
		"start:dev": "ts-node-dev --respawn --watch --exit-child --files server.ts",
		"start": "yarn build && (node build/server/server.js)",
		"user:create": "node build/server/src/scripts/createUser.js createuser",
		"test": "jest --config jest.config.json",
		"test:watch": "jest --config jest.config.json --watch",
		"test:coverage": "jest --config jest.config.json --coverage",
		"test:integration": "cross-env NODE_ENV=test jest --config jest.integration.config.json",
		"test:integration:watch": "cross-env NODE_ENV=test jest --config jest.integration.config.json --watch",
		"test:db:reset": "cross-env NODE_ENV=test prisma migrate reset --force",
		"db:test:start": "cross-env NODE_ENV=test docker-compose -f docker-compose.test.yml up -d",
		"db:test:stop": "cross-env NODE_ENV=test docker-compose -f docker-compose.test.yml down",
		"test:integration:setup": "yarn db:test:start && yarn test:db:reset",
		"test:integration:teardown": "yarn db:test:stop"
	},
	"dependencies": {
		"@prisma/client": "^4.1.0",
		"bcrypt": "^5.0.1",
		"cookie-parser": "^1.4.6",
		"dotenv": "^16.0.0",
		"express-mysql-session": "^2.1.7",
		"express-session": "^1.17.2",
		"fast-xml-parser": "^4.0.7",
		"multer": "^1.4.4",
		"passport": "^0.5.2",
		"passport-local": "^1.0.0",
		"prisma": "^4.1.0",
		"ts-node-dev": "^1.1.8",
		"yargs": "^17.5.1"
	}
}
